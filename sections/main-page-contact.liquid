{%- render 'main-page-contact' with section as section -%}

{%- comment -%} schematic writeCodeShort {%- endcomment -%}
{% schema %}
{
  "name": "Contact Section",
  "tag": "section",
  "class": "section-main-contact",
  "presets": [
    {
      "name": "Contact Section"
    }
  ],
  "settings": [
    {
      "type": "text",
      "id": "heading",
      "label": "Page Heading",
      "default": "Contact Us"
    },
    {
      "type": "select",
      "id": "heading_size",
      "label": "Heading Size",
      "options": [
        {
          "value": "text-2xl",
          "label": "Small"
        },
        {
          "value": "text-3xl",
          "label": "Medium"
        },
        {
          "value": "text-4xl",
          "label": "Large"
        }
      ],
      "default": "text-3xl"
    }
  ]
}
{% endschema %}