<div class="px-6 md:px-8 py-12">
  <div class="container">
    <div>
      {% for link in linklists.footer.links %}
        <a href="{{ link.url }}">
          {{ link.title }}
        </a>
      {% endfor %}
    </div>
  
    <small>
      &copy; {{ "now" | date: "%Y" }} Slayed Starter Theme
    </small>
  </div>
</div>
{%- comment -%} schematic {%- endcomment -%}
{% schema %}
{
  "name": "footer",
  "tag": "footer",
  "class": "footer",
  "presets": [
    {
      "name": "footer"
    }
  ],
  "settings": [
    {
      "type": "paragraph",
      "content": "t:sections.footer.settings.paragraph"
    }
  ]
}
{% endschema %}